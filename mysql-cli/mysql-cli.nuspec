    <tags>mysql command line client cli</tags>
    <bugTrackerUrl>https://bugs.mysql.com/</bugTrackerUrl>
    <docsUrl>https://dev.mysql.com/doc/refman/5.7/en/mysql.html</docsUrl>
    <mailingListUrl>https://forums.mysql.com/list.php?144</mailingListUrl>
    <packageSourceUrl>https://github.com/bazzilic/ChocolateyPackages</packageSourceUrl>    
  </metadata>
    <files>
        <file src="tools\**" target="tools" />
    </files>
</package>
